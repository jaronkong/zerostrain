            <kind>1</kind>
            <string>///Draw values

draw_set_color( c_white );
draw_set_alpha( image_alpha );
draw_set_halign( fa_left );
draw_set_valign( fa_middle );

var py = ( y + 16 );
var tSize = 15;

if ( sprite_exists( icon ) ) {
    var tIconScale = ( 1.4 * tSize / sprite_get_width( icon ) );
    var tIconWidth = ( sprite_get_width( icon ) * tIconScale );
    var tIconHeight = ( sprite_get_height( icon ) * tIconScale );
    draw_sprite_stretched_ext( icon, 0, x+m+tSize/2-tIconWidth/2, py-tIconHeight/2,
        tIconWidth, tIconHeight, c_white, image_alpha
    );
}
if ( sprite_exists( change ) ) {
    draw_sprite_stretched_ext( change, 0,
        x+width-tSize-m, py-tSize/2,
        tSize, tSize, c_white, image_alpha
    );
}

drawTextSize( x+m+tSize+m, py, title, font, 12 );
drawTextSize( x+width-m-tSize-getTextWidth( value, font, 10 )-m,
    py, value, font, 10
);

draw_set_color( c_white );
for ( var i = 0; i &lt; itemCount; ++i ) {
    py = ( y + ( ( i + 0.5 ) * 20 ) + 32 );
    drawTextSize( x+m+tSize+m, py, item[GeneDescGene.Title, i], font, 10 );
    drawTextSize(
        x+width-getTextWidth( item[GeneDescGene.Value, i], font, 10 )-m,
        py, item[GeneDescGene.Value, i], font, 10
    );
    var tChange = item[GeneDescGene.Change, i];
    if ( sprite_exists( tChange ) ) {
        draw_sprite_stretched_ext( tChange, 0, x+m, py-tSize/2,
            tSize, tSize, c_white, image_alpha
        );
    }
}
