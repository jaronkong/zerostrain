        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Check for exit

var tExit = false;
if ( keyboard_check_pressed( vk_space ) ) {
    tExit = true;
}
if ( gamepad_is_connected( 0 ) ) {
    if ( gamepad_button_check_pressed( 0, gp_shoulderrb ) ) {
        tExit = true;
    }
}

if ( tExit ) {
    if ( stateMergeComplete ) {
        if ( resultInfoAlpha &gt; 0.875 ) {
                event_user( 1 );
        }
